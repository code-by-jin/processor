-- null
DEPTH = 4096;
WIDTH = 32;

ADDRESS_RADIX = DEC;
DATA_RADIX = BIN;

CONTENT
BEGIN
    -- nop
0000 : 00000000000000000000000000000000;
    -- addi $1, $0, 65535      # r1 = 65535 = 0x0000FFFF
0001 : 00101000010000001111111111111111;
    -- sll $2, $1, 16			# r2 = r1 << 16 = 0xFFFF0000
0002 : 00000000100000100000100000010000;
    -- addi $3, $0, 8      #  8 = 0x00001000
0003 : 00101000110000000000000000001000;
    -- sll $4, $3, 16			# << 16 = 0x10000000
0004 : 00000001000001100000100000010000;
    -- add $5, $2, $4	# add ovf
0005 : 00000001010001000100000000000000;
    -- sll $6, $1, 15	# 0x7FFF8000
0006 : 00000001100000100000011110010000;
    -- addi $6, $6, 36727	# +0x7FFF
0007 : 00101001100011001000111101110111;
    -- addi $7, $2, 65534	#+0xFFFE
0008 : 00101001110001001111111111111110;
    -- sub $8, $7, $6    # 0xFFFFFFFE - 0x7FFFFFFF  sub ovf
0009 : 00000010000011100110000000000100;
    -- addi $9, $6, 1	# 0x7FFFFFFF+1=0x80000000 addi ovf
0010 : 00101010010011000000000000000001;
[0011 .. 4095] : 00000000000000000000000000000000;
END;
